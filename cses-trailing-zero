#include <bits/stdc++.h>

using namespace std;

void pair(int i, int y) {
		
}

int main(void) {
	
	int n;
	cin >> n;
	double x;
	int count=0;
	//int t=5;
	int i=1;
	do{
		x = n/pow(5, i);
		//cout << x <<endl;
		x = floor((double)x);
		//cout << x <<endl;
		count +=x;
		i++;
		
	}while(x);
	cout << count <<endl;
	return 0;
	
}
